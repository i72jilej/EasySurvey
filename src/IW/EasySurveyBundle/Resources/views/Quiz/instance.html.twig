{% extends "IWEasySurveyBundle::layout.html.twig" %}
{% block content %}
      <div class="page-header">          
        <h1>{% trans %}Instanciar encuesta{% endtrans %}</h1>
      </div>  
      <div class="page-content">  
          {% if quizs is empty %}
              {% trans %}Usted no dispone de ninguna encuesta instanciable{% endtrans %}
          {% else %}
          <table id="datetable">
              <thead>
              <tr>
                <th>Encuesta</th>
                <th>Proyecto</th>
                <th>Acci√≥n</th>
              </tr>
              </thead>
              <tbody>
              {% for data in quizs %}
              <tr>
                  <td> {{data.name}} </td>
                  <td> {{data.project}} </td>
                  <td> 
                      <a href="{{ path('iw_easy_survey_generate_instance', {'id': data.id})}}"> {% trans %}Instanciar{% endtrans %} </a>
                  </td>
              </tr>
              {% endfor %}
              </tbody>
          </table>
          {% endif %}
      </div>
{% endblock %}